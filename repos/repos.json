[
 {
  "kernelSource": {
    "name": "MIUI",
    "repo": "https://github.com/ztc1997/android_kernel_xiaomi_sm6150",
    "branch": "phoenix-r-oss",
    "device": "phoenix_ztc1997",
    "defconfig": "vendor/phoenix_ztc1997_defconfig"
  },
  "withKernelSU": true,
  "toolchains": [
    {
      "repo": "https://github.com/luyanci/kernel_bulid",
      "branch": "clang",
      "name": "clang",
      "binPath": ["toolclang/clang-r428724/bin"]
    },
    {
      "repo": "https://github.com/luyanci/kernel_bulid",
      "branch": "gcc",
      "name": "gcc",
      "binPath": ["toolgcc/bin"]
    },
    {
      "repo": "https://github.com/luyanci/kernel_bulid",
      "branch": "gcc",
      "name": "gcc-arm",
      "binPath": ["toolgcc/arm/bin"]
    }
  ],
  "ccache": false,
  "params": {
    "ARCH": "arm64",
    "CC": "clang",
    "externalCommand": {
      "CROSS_COMPILE": "aarch64-linux-android-",
      "CROSS_COMPILE_ARM32": "arm-linux-androideabi-",
      "CLANG_TRIPLE": "aarch64-linux-gnu-",
      "AR": "llvm-ar",
      "HOSTCC":"gcc",
      "HOSTCXX":"clang++",
      "NM":"llvm-nm" ,
      "OBJCOPY":"llvm-objcopy",
      "OBJDUMP":"llvm-objdump",
      "STRIP":"llvm-strip",
      "LD":"ld.lld"
    }
  },
  "AnyKernel3": {
    "use": true,
    "release": false,
    "repo": "https://github.com/luyanci/AnyKernel3",
    "branch": "master"
  }
}
